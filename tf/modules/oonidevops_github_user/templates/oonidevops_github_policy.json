{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ec2:Describe*",
                "s3:GetObject",
                "s3:ListBucket",
                "iam:GetRole",
                "iam:GetPolicy",
                "iam:ListRoles",
                "iam:ListPolicies",
                "rds:Describe*",
                "elasticache:Describe*",
                "route53:Get*",
                "lambda:Get*",
                "lambda:List*",
                "cloudfront:Get*",
                "cloudfront:List*",
                "sns:Get*",
                "sns:List*",
                "sqs:Get*",
                "sqs:List*",
                "logs:Get*",
                "logs:Describe*",
                "kinesis:Get*",
                "kinesis:List*",
                "apigateway:Get*",
                "apigateway:List*",
                "organizations:List*",
                "organizations:Describe*",
                "ecs:Describe*",
                "ecs:List*",
                "ecr:Describe*",
                "ecr:Get*",
                "events:Describe*",
                "events:List*",
                "stepfunctions:Describe*",
                "stepfunctions:Get*",
                "cloudwatch:Get*",
                "cloudwatch:List*",
                "dynamodb:Describe*",
                "dynamodb:Get*",
                "dynamodb:List*",
                "sagemaker:List*",
                "sagemaker:Describe*",
                "kms:Describe*",
                "kms:Get*",
                "kms:List*"
            ],
            "Resource": "*"
        }
    ]
}